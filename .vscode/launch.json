{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "gdb",
            "request": "launch",
            "name": "GDB32",
            "program": "${fileDirname}/${fileBasenameNoExtension}",
            "stopOnEntry": false,
            "preLaunchTask": "asm32"
        },
        {
            "type": "gdb",
            "request": "launch",
            "name": "GDB64",
            "program": "${fileDirname}/${fileBasenameNoExtension}",
            "stopOnEntry": false,
            "preLaunchTask": "asm64"
        },
        {
            "type": "gdb",
            "request": "launch",
            "name": "GAS",
            "program": "${fileDirname}/${fileBasenameNoExtension}",
            "stopOnEntry": false,
            "preLaunchTask": "gas"
        },
        {
            "type": "gdb",
            "request": "launch",
            "name": "GDB32+GCC",
            "program": "${fileDirname}/${fileBasenameNoExtension}",
            "stopOnEntry": false,
            "preLaunchTask": "asm32+gcc"
        },
        {
            "type": "gdb",
            "request": "launch",
            "name": "GDB64+GCC",
            "program": "${fileDirname}/${fileBasenameNoExtension}",
            "stopOnEntry": false,
            "preLaunchTask": "asm64+gcc"
        },
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            // Resolved by CMake Tools:
            "program": "${fileDirname}/${fileBasenameNoExtension}",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "externalConsole": true,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "gas"
        }
    ]
}